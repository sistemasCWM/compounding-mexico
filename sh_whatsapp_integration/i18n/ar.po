"\n"
"                        <t t-out=\"object.amount_total or ''\"/><t t-out=\"object.currency_id.symbol or ''\"/>\n"
"                        %0A%0A\n"
"                    </t>\n"
"                    <br/>\n"
"                    <t t-if=\"object.company_id.inv_send_pdf_in_message\">\n"
"                        *Click here to download Report  :* \n"
"\n"
